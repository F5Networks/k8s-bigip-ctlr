{
  "virtualServer": {
    "frontend": {
      "balance": "round-robin",
      "mode": "http",
      "partition": "kubernetes",
      "virtualAddress": {
        "bindAddr": "1.2.3.4",
        "port": 443
      },
      "sslProfile": {
        "f5ProfileName": "Common/testcert"
      },
      "sourceAddressTranslation": {
        "type": "automap"
      }
    },
    "backend": {
      "serviceName": "example-service",
      "servicePort": 443,
      "healthMonitors": [ {
        "send": "GET / HTTP/1.0\\r\\n\\r\\n",
        "interval": 30,
        "timeout": 20,
        "protocol": "http"
        }
      ]
    }
  }
}
