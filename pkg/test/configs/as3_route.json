{
  "class": "Application",
  "template": "shared",
  "openshift_insecure_routes": {
    "rules": [
      {
        "name": "openshift_route_f5demo_f5_demo_app_route",
        "conditions": [
          {
            "type": "httpHeader",
            "name": "host",
            "event": "request",
            "all": {
              "values": [
                "mysite.f5demo.com"
              ],
              "operand": "equals"
            }
          }
        ],
        "actions": [
          {
            "type": "forward",
            "event": "request",
            "select": {
              "pool": {
                "use": "openshift_f5demo_f5_demo_app_route"
              }
            }
          }
        ]
      }
    ],
    "strategy": "first-match",
    "class": "Endpoint_Policy"
  },
  "openshift_f5demo_f5_demo_app_route": {
    "loadBalancingMode": "least-connections-node",
    "members": [
      {
        "addressDiscovery": "static",
        "servicePort": 8080,
        "serverAddresses": []
      }
    ],
    "class": "Pool"
  },
  "https_ose_vserver": {
    "layer4": "tcp",
    "source": "0.0.0.0/0",
    "translateServerAddress": true,
    "translateServerPort": true,
    "class": "Service_HTTP",
    "profileHTTP": {
      "bigip": "/Common/http"
    },
    "profileTCP": {
      "bigip": "/Common/tcp"
    },
    "virtualAddresses": [
      "10.192.75.106%0"
    ],
    "virtualPort": 443,
    "snat": "auto"
  },
  "ose_vserver": {
    "layer4": "tcp",
    "source": "0.0.0.0/0",
    "translateServerAddress": true,
    "translateServerPort": true,
    "class": "Service_HTTP",
    "profileHTTP": {
      "bigip": "/Common/http"
    },
    "profileTCP": {
      "bigip": "/Common/tcp"
    },
    "virtualAddresses": [
      "10.192.75.106%0"
    ],
    "virtualPort": 80,
    "snat": "auto",
    "policyEndpoint": "/openshift/Shared/openshift_insecure_routes"
  }
}